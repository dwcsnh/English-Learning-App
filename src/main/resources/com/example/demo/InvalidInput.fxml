                <Font name="Cooper Black" size="14.0" />
            </font>
        </Label>
      <Label alignment="CENTER" layoutX="13.0" layoutY="54.0" prefHeight="34.0" prefWidth="223.0" text="Please type down 5 letter without" textFill="#4dc9c1">
         <font>
            <Font name="Cooper Black" size="12.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="13.0" layoutY="88.0" prefHeight="34.0" prefWidth="223.0" text="number or specific character!!" textFill="#4dc9c1">
         <font>
            <Font name="Cooper Black" size="12.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
